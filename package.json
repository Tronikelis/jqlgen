{
    "name": "jqlgen",
    "version": "2.0.2",
    "description": "A better way to generate jql",
    "type": "module",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.js",
    "files": [
        "./dist"
    ],
    "exports": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "default": "./dist/index.cjs"
    },
    "keywords": [
        "jql",
        "jira jql",
        "jql generator",
        "jql utility",
        "jqlgen",
        "atlassian",
        "jira",
        "jql.js"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Tronikelis/jqlgen.git"
    },
    "author": "Tronikelis",
    "license": "MIT",
    "devDependencies": {
        "@jest/globals": "29.7.0",
        "@types/jest": "29.5.7",
        "@types/node": "20.8.10",
        "eslint": "8.52.0",
        "eslint-config-tronikelis": "3.0.0",
        "jest": "29.7.0",
        "prettier": "3.0.3",
        "ts-jest": "29.1.1",
        "ts-node": "10.9.1",
        "tsup": "7.2.0",
        "typescript": "5.2.2"
    },
    "scripts": {
        "build": "tsup",
        "test": "jest",
        "prepublishOnly": "npm run test && npm run build"
    }
}
